{
    "name": "jinyerp/admin",
    "description": "Laravel Admin CRUD Generator with Livewire 3 - A comprehensive admin panel system for Laravel applications",
    "type": "library",
    "keywords": [
        "jinyerp",
        "admin",
        "laravel",
        "admin-panel",
        "crud",
        "livewire",
        "dashboard"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "JinyPHP Team",
            "email": "infohojin@gmail.com",
            "homepage": "https://github.com/jinyphp"
        }
    ],
    "homepage": "https://github.com/jinyphp/admin",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0",
        "livewire/livewire": "^3.0",
        "pragmarx/google2fa": "^8.0",
        "bacon/bacon-qr-code": "^2.0",
        "vonage/client": "^4.2",
        "google/recaptcha": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "laravel/pint": "^1.0",
        "laravel/telescope": "^5.0",
        "barryvdh/laravel-debugbar": "^3.0",
        "mockery/mockery": "^1.6",
        "fakerphp/faker": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "Jiny\\Admin\\": "src/"
        },
        "files": [
            "Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jiny\\Admin\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiny\\Admin\\JinyAdminServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/pint",
        "post-autoload-dump": [
            "@php artisan package:discover --ansi"
        ],
        "post-package-install": [
            "@php artisan migrate --force --ansi",
            "@php artisan admin:install --force --ansi"
        ],
        "post-package-update": [
            "@php artisan migrate --force --ansi"
        ]
    }
}
