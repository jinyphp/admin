{
    "name": "jiny/admin",
    "description": "Admin for JinyPHP",
    "keywords": [
        "Laravel", "jinyPHP", "framwork", "admin"
    ],
    "homepage": "http://www.jinyphp.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hojinlee",
            "email": "infohojin@gmail.com",
            "homepage": "http://www.jinyphp.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "czproject/git-php": "^4.3",

        "jiny/config": "^0.8",
        "jiny/permit": "^0.3.0",
        "jiny/wire-table": "^0.12"
    },
    "autoload": {
        "psr-4": {
            "Jiny\\Admin\\":"src/"
        },
        "files" : [
            "src/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jiny\\Admin\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiny\\Admin\\JinyAdminServiceProvider"
            ]
        }
    }
}
