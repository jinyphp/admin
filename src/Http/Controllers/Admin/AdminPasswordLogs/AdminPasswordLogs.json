{
    "controllerClass": "Jiny\\Admin\\App\\Http\\Controllers\\Admin\\AdminPasswordLogs\\AdminPasswordLogs",
    "title": "패스워드 보안 로그",
    "subtitle": "실패한 패스워드 시도 및 차단된 IP 모니터링",
    "description": "Password attempt logging and IP blocking management system.",
    "route": {
        "name": "admin.user.password.logs",
        "prefix": "admin\/user\/password\/logs",
        "actions": {
            "index": "index",
            "show": "show",
            "unblock": "unblock"
        }
    },
    "table": {
        "name": "admin_password_logs",
        "model": "\\Jiny\\Admin\\App\\Models\\AdminPasswordLog",
        "primaryKey": "id",
        "timestamps": true,
        "softDeletes": false,
        "where": {
            "default": [],
            "conditions": []
        },
        "casts": {
            "last_attempt_at": "datetime",
            "blocked_at": "datetime",
            "resolved_at": "datetime"
        }
    },
    "template": {
        "layout": "jiny-admin::layouts.admin",
        "index": "jiny-admin::template.index",
        "show": "jiny-admin::template.show"
    },
    "index": {
        "tableLayoutPath": "jiny-admin::template.livewire.admin-table",
        "tablePath": "jiny-admin::admin.admin_password_logs.table",
        "searchLayoutPath": "jiny-admin::template.livewire.admin-search",
        "searchFormPath": "jiny-admin::admin.admin_password_logs.search",
        "searchable": [
            "email",
            "ip_address",
            "browser"
        ],
        "sortable": [
            "id",
            "email",
            "ip_address",
            "attempt_count",
            "status",
            "last_attempt_at",
            "blocked_at"
        ],
        "filterable": [
            "status"
        ],
        "heading": {
            "title": "패스워드 보안 로그",
            "description": "Monitor and manage failed password attempts and blocked IPs"
        },
        "features": {
            "enableCreate": false,
            "enableDelete": true,
            "enableEdit": false,
            "enableSearch": true,
            "enableSort": true,
            "enablePagination": true,
            "enableBulkActions": true,
            "enableExport": true,
            "enableImport": false,
            "enableStatusToggle": false,
            "enableSettingsDrawer": true,
            "enableUnblock": true
        },
        "pagination": {
            "perPage": "10",
            "perPageOptions": [
                10,
                20,
                50,
                100
            ]
        },
        "sorting": {
            "default": "last_attempt_at",
            "direction": "desc"
        },
        "search": {
            "placeholder": "Search by email, IP address, or browser...",
            "debounce": 300
        },
        "filters": {
            "status": {
                "label": "Status",
                "options": {
                    "": "All Status",
                    "failed": "Failed Attempts",
                    "blocked": "Blocked IPs",
                    "resolved": "Resolved"
                }
            },
            "attempt_count": {
                "label": "Attempt Count",
                "type": "range",
                "min": 1,
                "max": 100
            },
            "date_range": {
                "label": "Date Range",
                "type": "daterange"
            }
        },
        "table": {
            "columns": {
                "checkbox": {
                    "visible": true,
                    "width": "50px"
                },
                "id": {
                    "label": "ID",
                    "visible": false,
                    "sortable": true,
                    "width": "80px"
                },
                "email": {
                    "label": "Email",
                    "visible": true,
                    "sortable": true,
                    "searchable": true,
                    "linkRoute": "admin.users.show",
                    "linkParam": "user_id"
                },
                "ip_address": {
                    "label": "IP Address",
                    "visible": true,
                    "sortable": true,
                    "searchable": true
                },
                "browser": {
                    "label": "Browser",
                    "visible": true,
                    "responsive": "lg",
                    "truncate": 30
                },
                "attempt_count": {
                    "label": "Attempts",
                    "visible": true,
                    "sortable": true,
                    "badge": true,
                    "badgeColors": {
                        "1-2": "gray",
                        "3-4": "yellow",
                        "5+": "red"
                    }
                },
                "status": {
                    "label": "Status",
                    "visible": true,
                    "sortable": true,
                    "badge": true,
                    "badgeColors": {
                        "failed": "yellow",
                        "blocked": "red",
                        "resolved": "green"
                    }
                },
                "last_attempt_at": {
                    "label": "Last Attempt",
                    "visible": true,
                    "sortable": true,
                    "format": "Y-m-d H:i:s",
                    "responsive": "md"
                },
                "blocked_at": {
                    "label": "Blocked At",
                    "visible": false,
                    "sortable": true,
                    "format": "Y-m-d H:i:s"
                },
                "actions": {
                    "label": "Actions",
                    "visible": true,
                    "width": "150px"
                }
            }
        },
        "actions": {
            "unblock": {
                "label": "Unblock",
                "icon": "unlock",
                "class": "text-green-600 hover:text-green-900",
                "confirmation": {
                    "title": "IP 차단 해제",
                    "message": "Are you sure you want to unblock this IP address?",
                    "type": "warning"
                }
            },
            "bulkUnblock": {
                "label": "Unblock Selected",
                "icon": "unlock",
                "class": "bg-green-600 hover:bg-green-700 text-white"
            }
        }
    },
    "show": {
        "heading": {
            "title": "Password Log Details",
            "description": "View detailed password attempt information"
        },
        "enableEdit": false,
        "enableDelete": true,
        "enableCreate": false,
        "enableListButton": true,
        "enableSettingsDrawer": true,
        "enableUnblock": true,
        "showLayoutPath": "jiny-admin::template.livewire.admin-show",
        "showPath": "jiny-admin::admin.admin_password_logs.show",
        "sections": {
            "basic": {
                "title": "Basic Information",
                "fields": [
                    "email",
                    "ip_address",
                    "status",
                    "attempt_count"
                ]
            },
            "browser": {
                "title": "Browser Information",
                "fields": [
                    "browser",
                    "user_agent",
                    "device",
                    "platform"
                ]
            },
            "timestamps": {
                "title": "Timeline",
                "fields": [
                    "first_attempt_at",
                    "last_attempt_at",
                    "blocked_at",
                    "resolved_at"
                ]
            },
            "resolution": {
                "title": "Resolution Details",
                "fields": [
                    "resolved_by",
                    "resolution_notes"
                ],
                "condition": "status == 'resolved'"
            }
        },
        "display": {
            "datetimeFormat": "Y-m-d H:i:s",
            "booleanLabels": {
                "true": "Yes",
                "false": "No"
            }
        }
    },
    "destroy": {
        "enableSoftDelete": false,
        "enableTransaction": true,
        "requireConfirmation": true,
        "confirmation": {
            "message": "Are you sure you want to delete this password log entry?"
        },
        "messages": {
            "success": "Password log entry deleted successfully.",
            "error": "Error deleting password log entry: %s",
            "confirmRequired": "Delete confirmation required."
        }
    },
    "messages": {
        "unblocked": "IP address has been unblocked successfully.",
        "unblockFailed": "Failed to unblock IP address.",
        "bulkUnblocked": "%d IP addresses have been unblocked.",
        "deleted": "Password log entry deleted successfully.",
        "error": "An error occurred while processing your request."
    },
    "currentRoute": "admin.user.password.logs"
}